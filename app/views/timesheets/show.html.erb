<h1><%= @timesheet.start_date.strftime('%B %Y')%></h1>

<table class="table">
  <thead>
    <tr>
      <th scope="col">Date</th>
      <th scope="col">Hours</th>
      <th scope="col">Task</th>
      <th scope="col">Subtotal</th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <% @timesheet.timestamps.each do |timestamp| %>
        <% unless timestamp.end_time.nil? %>
          <td><%= timestamp.date %></td>
          <td><%= timestamp.hours %></td>
          <td><%= timestamp.task_description %></td>
          <td><%= timestamp.subtotal %></td>
        <% end %>
      <% end %>
    </tr>
  </tbody>
</table>
